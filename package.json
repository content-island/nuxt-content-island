{
  "name": "@content-island/nuxt",
  "version": "0.2.1",
  "description": "Content Island - Nuxt module",
  "private": false,
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "module": "./dist/module.mjs",
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.mts",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "scripts": {
    "start": "npm run start:playground",
    "start:playground": "nuxi dev playground",
    "build": "nuxt-module-build build",
    "test": "vitest",
    "changeset": "changeset",
    "publish-packages": "changeset publish",
    "prelocal:publish": "npm run build && cd ./local-npm-registry && npm start",
    "local:publish": "changeset version && node local-npm-registry/publish.js changeset publish",
    "prepare": "nuxt-module-build prepare && husky || echoF 'Husky is not installed'",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@content-island/api-client": "^0.16.0",
    "@nuxt/kit": "^4.2.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@nuxt/module-builder": "^1.0.2",
    "@types/node": "^25.0.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "nuxt": "^4.2.2",
    "prettier": "^3.7.4",
    "pretty-quick": "^4.2.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16",
    "vue-tsc": "^3.2.2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/content-island/nuxt-content-island.git"
  },
  "keywords": [
    "content-island",
    "nuxt",
    "module",
    "content",
    "island"
  ],
  "author": "Lemoncode",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/content-island/nuxt-content-island/issues"
  },
  "homepage": "https://github.com/content-island/nuxt-content-island#readme"
}
